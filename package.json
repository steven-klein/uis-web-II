{
  "name": "",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all --serial clean copy js css",
    "clean": "npm-run-all --parallel clean:*",
    "clean:assets": "del-cli httpdocs/assets",
    "copy": "npm-run-all --parallel copy:*",
    "copy:img": "copy 'resources/assets/img/**/*.+(jpg|jpeg|gif|png|svg)' httpdocs/assets/img",
    "css": "postcss -c postcss.config.js",
    "deploy:prod": "cross-env NODE_ENV=production npm run build && brkstn-sync prod up",
    "deploy:dev": "cross-env NODE_ENV=production npm run build && brkstn-sync dev up",
    "js": "rollup -c rollup.config.js",
    "postversion": "git push && git push --tags",
    "serve": "browser-sync start -c browsersync.config.js",
    "test": "cross-env NODE_ENV=production npm run build",
    "watch": "npm-run-all clean copy js css --parallel \"js -- --watch\" \"css -- --watch\" serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.18.5",
    "copy": "^0.3.0",
    "cross-env": "^3.1.4",
    "cssnano": "^3.9.1",
    "del-cli": "^0.2.1",
    "dotenv": "^4.0.0",
    "npm-run-all": "^4.0.0",
    "postcss": "^5.2.8",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "postcss-reporter": "^3.0.0",
    "rollup": "^0.39.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.1.0"
  },
  "dependencies": {
    "core-js": "^2.4.1"
  },
  "browserslist": [
    "last 4 versions",
    "ie >= 9",
    "ios >= 7",
    "safari >= 5.1",
    "android >= 4",
    "last 4 and_chr versions",
    "last 4 and_ff versions",
    "last 1 op_mini version",
    "last 4 and_uc versions",
    "last 4 op_mob versions",
    "last 4 Samsung versions",
    "not ie <= 8"
  ]
}
